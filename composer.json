{
    "require": {
        "ramsey/uuid": "^4.7",
        "vlucas/phpdotenv": "^5.5",
        "psr/container": "^2.0",
        "symfony/mailer": "^6.3",
        "doctrine/dbal": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    }
}
