{
    "require": {
        "ramsey/uuid": "^4.7",
        "vlucas/phpdotenv": "^5.5",
        "doctrine/dbal": "^3.3",
        "doctrine/orm": "^2.4",
        "symfony/cache": "^6.3",
        "doctrine/migrations": "3.6",
        "illuminate/database": "^10.14",
        "illuminate/events": "^10.14",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^7.7",
        "twig/twig": "^3.6",
        "twig/intl-extra": "^3.4",
        "slim/slim": "4.*",
        "slim/psr7": "^1.6",
        "slim/twig-view": "^3.3",
        "php-di/php-di": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    }
}
